
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>_SelleryLayout</title>

    <link href="~/Areas/Sellers/Content/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
        
    <link href="~/Areas/Sellers/Content/css/sb-admin-2.min.css" rel="stylesheet" />

</head>
<body>

    <div id="wrapper">
        @Html.Partial("_SelleryDivider")
        <div id="content-wrapper" class="d-flex flex-column">
            <div id="content">
                @Html.Partial("_SelleryNavbar")

                <div class="container-fluid">
                    @RenderBody()
                </div>
            </div>

            @Html.Partial("_SelleryFooter")
        </div>

    </div>


    <script src="~/Areas/Sellers/Content/vendor/jquery/jquery.min.js"></script>
    <script src="~/Areas/Sellers/Content/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/Areas/Sellers/Content/vendor/jquery-easing/jquery.easing.min.js"></script>
    <script src="~/Areas/Sellers/Content/js/sb-admin-2.min.js"></script>
    <script src="~/Areas/Sellers/Content/vendor/chart.js/Chart.min.js"></script>
    <script src="~/Areas/Sellers/Content/js/demo/chart-area-demo.js"></script>
    <script src="~/Areas/Sellers/Content/js/demo/chart-pie-demo.js"></script>
</body>
</html>
